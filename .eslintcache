       console.error(err);\n        setIsLoading(false);\n      });\n  };\n\n  const handleDeletePatient = () => {\n    db.collection(`doctors/${auth.uid}/patients`)\n      .doc(id)\n      .delete()\n      .then(() => {\n        console.log(\"Document successfully deleted!\");\n        history.push(ROUTES.PATIENTS);\n      })\n      .catch((err) => console.error(err));\n  };\n\n  useEffect(() => {\n    getPatientDetails();\n  }, []);\n\n  return (\n    <div className={classes.container}>\n      {!isLoading ? (\n        <>\n          <Grid container spacing={3}>\n            <Grid item xs={12} md={12} lg={12}>\n              {\" \"}\n              <div>\n                <div className={classes.btnGroup}>\n                  {\" \"}\n                  <Button\n                    className={classes.btn}\n                    variant=\"contained\"\n                    color=\"primary\"\n                    endIcon={<PrintIcon />}\n                    onClick={handlePrint}\n                  >\n                    Print\n                  </Button>\n                  <Button\n                    className={classes.btn}\n                    startIcon={<DeleteIcon />}\n                    variant=\"outlined\"\n                    color=\"primary\"\n                    onClick={handleDeletePatient}\n                  >\n                    Delete\n                  </Button>\n                </div>\n              </div>\n            </Grid>\n          </Grid>\n\n          <Grid ref={patientInfo} container spacing={3}>\n            <Grid item xs={12}>\n              <Link\n                style={{\n                  textDecoration: \"none\",\n                  color: \"inherit\",\n                }}\n                to={`${ROUTES.MEDICAL_HISTORY}/${id}`}\n              >\n                <ModernCard>\n                  {\" \"}\n                  <Typography variant=\"h6\" gutterBottom>\n                    Patient Information\n                  </Typography>\n                  <Grid container justify=\"space-between\">\n                    <Grid item>\n                      {\" \"}\n                      <Typography\n                        className={classes.subHeading}\n                        variant=\"subtitle2\"\n                        gutterBottom\n                      >\n                        Full Name :{\" \"}\n                        <span className={classes.info}>\n                          {(patient.patientInformation &&\n                            patient.patientInformation.fullName) ||\n                            \"--\"}\n                        </span>\n                      </Typography>\n                      <Typography\n                        className={classes.subHeading}\n                        variant=\"subtitle2\"\n                        gutterBottom\n                      >\n                        Gender :{\" \"}\n                        <span className={classes.info}>\n                          {(patient.patientInformation &&\n                            patient.patientInformation.gender) ||\n                            \"--\"}\n                        </span>\n                      </Typography>\n                      <Typography\n                        className={classes.subHeading}\n                        variant=\"subtitle2\"\n                        gutterBottom\n                      >\n                        Address :{\" \"}\n                        <span className={classes.info}>\n                          {(patient.patientInformation &&\n                            patient.patientInformation.address) ||\n                            \"--\"}\n                        </span>\n                      </Typography>\n                      <Typography\n                        className={classes.subHeading}\n                        variant=\"subtitle2\"\n                        gutterBottom\n                      >\n                        Phone Number:{\" \"}\n                        <span className=